    "debug": "^3.1.0",
    "interface-connection": "~0.3.2",
    "lodash.includes": "^4.3.0",
    "mafmt": "^4.0.0",
    "multiaddr": "github:mkg20001/js-multiaddr#znjs-relay-test",
    "once": "^1.4.0",
    "peer-id": "~0.10.6",
    "peer-info": "~0.11.6",
